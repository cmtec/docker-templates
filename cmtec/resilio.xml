<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>Web:</Category>
  <Name>resilio</Name>
  <Description>
	All Your Data, Across All Your Devices
  </Description>
  <Registry>https://registry.hub.docker.com/u/cmtec/resilio/</Registry>
  <Repository>cmtec/resilio</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>8888</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>55555</HostPort>
        <ContainerPort>55555</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/resilio</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
  </Data>
  <WebUI>https://[IP]:[PORT:8888]/</WebUI>
  <Icon>https://github.com/cmtec/docker-templates/raw/master/images/resilio.png</Icon>
  <Banner></Banner>
</Container>
