<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>VersionControl:</Category>
  <Name>gogs</Name>
  <Description>
  A painless self-hosted Git service.
  [br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
  [b]/data[/b] : Storing all gogs config files and git repositories[br]
  </Description>
  <Registry>https://hub.docker.com/r/gogs/gogs/</Registry>
  <Repository>gogs/gogs</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>10022</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
  </Data>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <Icon>https://try.gogs.io/img/favicon.png</Icon>
  <Banner></Banner>
</Container>
