<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>Network:</Category>
  <Name>tftp</Name>
  <Description>
  </Description>
  <Registry>https://registry.hub.docker.com/u/cmtec/tftp/</Registry>
  <Repository>cmtec/tftp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>69</HostPort>
        <ContainerPort>69</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
  </Data>
  <Icon>http://img11.deviantart.net/fa7b/i/2012/262/1/3/tftpserver_flurry_ios_style_icon_by_flakshack-d5f885v.png</Icon>
  <Banner></Banner>
</Container>
