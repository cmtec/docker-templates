<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>HomeAutomation:</Category>
  <Name>cmcs</Name>
  <Description>
    Private docker!
  </Description>
  <Registry>https://registry.hub.docker.com/u/cmtec/cmcs/</Registry>
  <Repository>cmtec/cmcs</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8086</HostPort>
        <ContainerPort>8081</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>PYTHONUNBUFFERED</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Data>
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
  </Data>
  <WebUI>http://[IP]:[PORT:8086]/</WebUI>
  <Icon>https://lh4.googleusercontent.com/-Dwr28KLpNuY/UmTOmMLSBSI/AAAAAAAAACw/YtYH570unOc/s250-no/cmtec_logo_big.png</Icon>
  <Banner></Banner>
  <ExtraParams>--device /dev/ttyACM0</ExtraParams>
</Container>
