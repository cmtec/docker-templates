<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>Web:</Category>
  <Name>freeradius</Name>
  <Description>
  </Description>
  <Registry>https://registry.hub.docker.com/u/cmtec/freeradius/</Registry>
  <Repository>cmtec/freeradius</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1812</HostPort>
        <ContainerPort>1812</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>1813</HostPort>
        <ContainerPort>1813</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>18120</HostPort>
        <ContainerPort>18120</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>18120</HostPort>
        <ContainerPort>18120</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
     <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume> 
  </Data>
  <WebUI></WebUI>
  <Icon>https://raw.githubusercontent.com/cmtec/docker-templates/master/images/Freeradius_logo.png</Icon>
  <Banner></Banner>
</Container>
